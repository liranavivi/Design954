{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Extensions.Http": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "Shared.MCP": "Information"
    }
  },
  "McpServer": {
    "Http": {
      "Port": 8080,
      "Host": "0.0.0.0"
    },
    "EnableExperimental": false
  },
  "OpenTelemetry": {
    "UseConsoleExporter": true,
    "Endpoint": "http://otel-collector:4317",
    "HealthEndpoint": "http://otel-collector:8081"
  },
  "ManagerUrls": {
    "Schema": "http://manager-schema:5160"
  }
}
